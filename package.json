{
  "name": "jest-coverage-badges-ts",
  "version": "0.1.6",
  "description": "Create a group of coverage badges from jest",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophe77/jest-coverage-badges-ts.git"
  },
  "keywords": [
    "coverage",
    "badge",
    "jest",
    "istanbul"
  ],
  "author": "Christophe bellec",
  "main": "dist/cli.js",
  "bin": "dist/cli.js",
  "types": "dist/*.ts",
  "engines": {
    "node": ">=6.11",
    "npm": ">=5.3"
  },
  "scripts": {
    "dev": "concurrently -k -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,green\" \"tsc --watch\" \"nodemon dist/cli.js\"",
    "start": "tsc && node dist/cli.js",
    "build": "tsc",
    "test": "jest --coverage",
    "prepublish": "yarn build",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.3.0",
    "jest": "^27.3.1",
    "nodemon": "2.0.15",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "files": [
    "dist/*.js"
  ],
  "license": "MIT"
}
